{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "X402_NETWORK": "eip155:8453",
    "X402_FACILITATOR_URL": "https://api.cdp.coinbase.com/platform/v2/x402"
  }
}
